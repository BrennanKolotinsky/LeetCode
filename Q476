class Solution {
    public int findComplement(int num) {
        
        int n = 0;        
        int mask = 1;
        while (num > 0) {
            
            if (num % 2 == 0) {
                n += mask;
            }
            
            num >>= 1;
            mask <<= 1;
        }
        
        return n;
    }
}