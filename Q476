class Solution {
    public int findComplement(int num) {
        
        int n = 0;        
        int cnt = 1;
        while (num > 0) {
            
            if (num % 2 == 0) {
                n += cnt;
            }
            
            num >>= 1;
            cnt *= 2;
        }
        
        return n;
    }
}