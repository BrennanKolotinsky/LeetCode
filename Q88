class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        
        if (n == 0) return;
        
        int i;
        for (i = 0; i < m; i++) {
            if (nums1[i] > nums2[0]) {
                int tmp = nums1[i];
                nums1[i] = nums2[0];
                nums2[0] = tmp;
                Arrays.sort(nums2);
            }
        } 
                
        for (int j = 0; j < n; j++) {
            nums1[i + j] = nums2[j];
        }
    }
}